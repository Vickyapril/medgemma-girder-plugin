# Hugging Face Configuration (for MedGemma models)
HUGGINGFACE_TOKEN=your_huggingface_token_here

# MedGemma Model Selection
# Options:
# - google/medgemma-27b-text-it (text-only, recommended for metadata analysis)
# - google/medgemma-4b-multimodal-it (multimodal, for image analysis)
MEDGEMMA_MODEL_NAME=google/medgemma-27b-text-it

# Use Hugging Face Inference API (True) or load model locally (False)
USE_INFERENCE_API=true

# Optional: Custom settings
NUM_SLICES=5
OUTPUT_DIR=./output
